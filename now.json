{
  "version": 2,
  "name": "ff-backend",
  "builds": [
    { "src": "package.json", "use": "@now/static-build" }
  ],
  "routes": [
    {"src": "^/static/(.*)", "dest": "/static/$1"},
    {"src": "^/favicon.ico", "dest": "/favicon.ico"},
    {"src": "^/(.*)", "dest": "/index.html"}
  ],
  "alias": "admin.theformer.faith"
}